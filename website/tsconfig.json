{
  "compilerOptions": {
    "outDir": "dist",
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "allowJs": true,
    "strict": true,
    "jsx": "preserve",
    "baseUrl": ".",
    "types": ["vuetify"],
    "paths": {
      "@theme/*": [".vitepress/theme/*"],
      "@playground/*": [".vitepress/playground/*"],
      "@shared/*": [".vitepress/shared/*"]
    }
  },
  "include": ["env.d.ts", ".vitepress/**/*"]
}
